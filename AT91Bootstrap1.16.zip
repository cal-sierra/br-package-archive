        /*Function to read the cookie value for given name check if cookie holds any value then return its value else return empty.*/
        function readCookie(cname) {
            if (document.cookie.length > 0) {
                cstart = document.cookie.indexOf(cname + "=");
                if (cstart != -1) {
                    cstart = cstart + cname.length + 1;
                    cend = document.cookie.indexOf("|", cstart);
                    if (cend == -1) cend = document.cookie.length;
                    return unescape(document.cookie.substring(cstart, cend));
                }
            }
            return "";
        }
        /*Function to set the cookie for given name, the cookie expires based on the value of the expiredays*/
        function setCookie(cname, value, expiredays) {
            var exdate = new Date();
            exdate.setDate(exdate.getDate() + expiredays);
            document.cookie = cname + "=" + escape(value) + ((expiredays == null) ? "" : "|expires=" + exdate.toUTCString()) + ";domain=.atmel.com;path=/";
        }
        var timerId = null, timeout = 5;
        /*Function to get eloqua cust id*/
        function WaitUntilCustomerGUIDIsRetrieved() {
            if (!!(timerId)) {
                if (timeout == 0)
                    return;
                if (typeof this.GetElqCustomerGUID === 'function') {
                    setCookie('elqcg', GetElqCustomerGUID());
                    return;
                }
                timeout -= 1;
            }
            timerId = setTimeout("WaitUntilCustomerGUIDIsRetrieved()", 500);
            return;
        }

        function TrackCart(inputCtrlName, partNumber, returnUrl,frmName) {
            var elmQty = document.getElementById(inputCtrlName);
            var elmHdnRetUrl = document.getElementById(returnUrl);
            var elmFrm = document.getElementById(frmName);
            var cartUrl = "https://buy.atmel.com/Cart";
            var type = jQuery('#hdnComponentType').text();
            if (typeof (type) == "undefined")
                type = "Device";
            TrackLink("Buy", type, partNumber);

            if (elmQty != null && (elmQty.value === '' || elmQty.value === '0')) {
                elmQty.value = '1';
            }
            if (elmHdnRetUrl != null) {
                elmHdnRetUrl.value = encodeURIComponent(window.location.pathname);
            }
            if (elmFrm != null) {
                elmFrm.action = cartUrl;
                elmFrm.submit();
            }
        }

        function DisplayDistiLinks() {
            var data = jQuery('#hdnPipeOrderingCodes').text();
            if (typeof (data) != "undefined") {
                data = jQuery.trim(data);
                var spltData = data.split("|");
                for (i = 0; i < spltData.length; i++) {
                    if (typeof (spltData[i]) != "undefined" && jQuery.trim(spltData[i]) !== "") {
                        var atmelPartNumber = jQuery.trim(spltData[i]);
                        var elemDistiLink = document.getElementById(atmelPartNumber + "_disti_link");
                        var elemPrice = document.getElementById(atmelPartNumber + "_price");
                        var elemDivCart = document.getElementById(atmelPartNumber + "_cartBlock");
                        var elmDisBlock = document.getElementById(atmelPartNumber + "_distiBlock");
                        if (elemDivCart != null)
                            elemDivCart.style.display = "none";
                        if (elmDisBlock != null)
                            elmDisBlock.style.display = "block";
                        if (elemDistiLink != null)
                            elemDistiLink.style.display = "block";
                        if (elemPrice != null) {
                            while (elemPrice.firstChild) {
                                elemPrice.removeChild(elemPrice.firstChild);
                            }
                            elemPrice.appendChild(document.createTextNode('Varies'));
                        }
                    }
                }
            }
        }
        
