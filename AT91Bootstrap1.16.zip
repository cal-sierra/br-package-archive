            TrackLink('Downloads', assetType, assetTitle);
            if (typeof (targetUri) == "undefined" || targetUri == "")
                return true;
            if (typeof (reqRegistn) != "undefined" && reqRegistn == "Yes")
                return true;
            jQuery.ajax({
                Type: "GET",
