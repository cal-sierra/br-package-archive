                if (e.which !== 8 && e.which !== 0 && (e.which < 48 || e.which > 57)) return false;
            });

            if (typeof (jQuery('#hdnOrderingCodeMessage')) != "undefined")
                jQuery("#hdnOrderingCodeMessage").css("display", "block");
            var data = jQuery('#hdnOrderingCodes').text();
            var type = jQuery('#hdnComponentType').text();
            if (typeof (type) == "undefined")
                type = "Device";

            if (typeof (type) != "undefined" && type === "tool") {
                var spltData = data.split("|");
                var dataTmp="";
                for (i = 0; i < spltData.length; i++) {
                    if (jQuery.trim(spltData[i]) !== "") {
                        if (dataTmp === "")
                            dataTmp = dataTmp + jQuery.trim(spltData[i]);
                        else
                            dataTmp = dataTmp + "|" + jQuery.trim(spltData[i]);
                    }
                }
                data = dataTmp;
            }

            if (typeof (data) != "undefined" && data !== '') {
                var spinner = document.getElementById("spinner");
                if (spinner != null)
                    spinner.style.display = "block";
                jQuery.ajax({
                    Type: "GET",
                    url: "http://www.atmel.com/system/GetOrderingDetails.ashx",
                    dataType: 'jsonp',
                    data: 'data=' + jQuery.trim(data) + '&type=' + type,
                    timeout: 20000,
                    success: function(data) {
                        var sResult = eval(data);
                        if (typeof (sResult) != "undefined") {
                            if (sResult.Status === "OK") {
                                if (spinner != null)
                                    spinner.style.display = "none";
                                for (var i=0; i< sResult.OrderCodes.length; i++) {
                                    var dkPartNumber = sResult.OrderCodes[i].DkPartNumber;
                                    var atmelPartNumber = sResult.OrderCodes[i].AtmelPartNumber;
                                    var quantityOnHand = sResult.OrderCodes[i].QuantityOnHand;
                                    var unitPrice = sResult.OrderCodes[i].UnitPrice;
                                    var packaging = sResult.OrderCodes[i].Packaging;
                                    var breakQuantity = sResult.OrderCodes[i].BreakQuantity;
                                    
                                    if (typeof (atmelPartNumber) != "undefined") {
                                        var elemLimit = document.getElementById(atmelPartNumber + "_limit");
                                        var elemQty = document.getElementById(atmelPartNumber + "_qty");
                                        var elemLink = document.getElementById(atmelPartNumber + "_link");
                                        var elemDistiLink = document.getElementById(atmelPartNumber + "_disti_link");
                                        var elemPrice = document.getElementById(atmelPartNumber + "_price");
                                        var elemDivCart = document.getElementById(atmelPartNumber + "_cartBlock");
                                        var elemBuyable = document.getElementById(atmelPartNumber + "_buyable");
                                        var buyFlag = "";
                                        if(elemBuyable != null)
                                            buyFlag =  typeof (elemBuyable.textContent) != "undefined" ? jQuery.trim(elemBuyable.textContent) : jQuery.trim(elemBuyable.innerText);
                                        if (elemLimit != null && typeof (quantityOnHand) != "undefined" && elemBuyable != null && buyFlag === "Yes") {
                                            while (elemLimit.firstChild) {
                                                elemLimit.removeChild(elemLimit.firstChild);
                                            }
                                            elemLimit.appendChild(document.createTextNode(quantityOnHand));
                                            elemLimit.style.display = "block";
                                            if (elemLink != null)
                                                elemLink.style.display = "block";
                                            if (elemQty != null)
                                                elemQty.style.display = "block";
                                            if (elemDistiLink != null)
                                                elemDistiLink.style.display = "none";
                                            if (elemDivCart != null)
                                                elemDivCart.style.display = "block";
                                        }
                                        if (elemPrice != null && typeof (unitPrice) != "undefined" && elemBuyable != null && buyFlag === "Yes") {
                                            while (elemPrice.firstChild) {
                                                elemPrice.removeChild(elemPrice.firstChild);
                                            }
                                            elemPrice.appendChild(document.createTextNode(breakQuantity + ' @ USD ' + unitPrice + ' each'));
                                        }
                                    }
                                }
                                return true;
                            } else {
                                if (spinner != null)
                                    spinner.style.display = "none";
                                DisplayDistiLinks();
                                return false;
                            }
                        }
                    },
                    error: function (data) {
                        if (spinner != null)
                            spinner.style.display = "none";
                        DisplayDistiLinks();
                        return;
                    }
                });
            }
        });
    </script>  

