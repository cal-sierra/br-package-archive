        function openFavoritebox(obj, pageName, pageUrl) {
            jQuery("#overlayFavorite").show();
            jQuery("#overlayFavorite").append("<span>Add To Favorites</span>");
            jQuery("#overlayFavorite").append("<p>Name : <input type='text' id='txtlnkName' name='txtlnkName'   maxlength='2000' size='28'/></p>");
            jQuery("#overlayFavorite").append("<p style='float:right; ' >");
            jQuery("#overlayFavorite").append("<input type='button' id='SaveFavorite' name='submit'  value='Save' />");
            jQuery("#overlayFavorite").append("<input type='button' id='CancelFavorite' name='cancel' value='Cancel' />");
            jQuery("#overlayFavorite").append("</p>");
            document.getElementById('fade').style.display = 'block';
            document.getElementById('txtlnkName').value = typeof (pageName) != "undefined" ? pageName : document.getElementsByTagName("title")[0].innerHTML.replace("- Atmel Corporation", "");

            var x = jQuery("#" + obj).offset();
            //Set the popup window under the favorite link 
            jQuery("#overlayFavorite").append("<img id='overlayArrDn.png'  style='top:-13px; margin:0 15px 0 0;right:0px; position:absolute;'  src='/Images/overlayArrDn.png'/>");
            jQuery("#overlayFavorite").css('top', x.top + jQuery("#" + obj).height() + 10);
            jQuery("#overlayFavorite").css('left', x.left - jQuery("#overlayFavorite").width() + jQuery("#" + obj).width());
            jQuery("#overlayFavorite").fadeIn(200);
            jQuery("html, body").animate({ scrollTop: jQuery('#' + obj).offset().top }, 'normal');
            jQuery("#SaveFavorite").focus();

            jQuery("#SaveFavorite").click(function () {
                AddToFavourites(pageName, pageUrl, function (retResult) {
                    if (retResult) {
                        jQuery("#" + obj).hide();
                        jQuery("#" + obj + "Exists").show();
                        jQuery("#overlayFavorite").hide();
                        document.getElementById('fade').style.display = 'none'; //background disable
                    }
                    else {
                        jQuery("#overlayFavorite").hide();
                        document.getElementById('fade').style.display = 'none'; //background disable                    
                    }
                    jQuery("#overlayFavorite").empty();
                });
            })
            jQuery("#CancelFavorite").click(function () {
                document.getElementById('overlayFavorite').style.display = 'none';
                document.getElementById('fade').style.display = 'none';
                jQuery("#overlayFavorite").empty();
            })
            jQuery("#txtlnkName").keypress(function (event) {
                if (event.which == 13) {
                    jQuery('#SaveFavorite').click();
                    event.preventDefault();
                }
            });
        }     
    </script>
    <script type="text/javascript">
        function AddToFavourites(pageTitle, pageUrl, callback) {
            var lnkName = jQuery("#txtlnkName").val();
            var lnkUrl = typeof (pageUrl) != "undefined" ? pageUrl : encodeURIComponent(document.URL);
            if (lnkUrl == "")
                return false;
            if (lnkName == null || lnkName.length <= 0)
                lnkName = document.getElementsByTagName("title")[0].innerHTML.replace("- Atmel Corporation", "");
            jQuery.ajax({
                Type: "GET",
                url: "https://secure.atmel.com/secure/AddFavorites.ashx",
                dataType: 'jsonp',
                data: 'lnkName=' + lnkName + '&lnkUrl=' + lnkUrl,
                timeout: 9000,
                //if response is successfull, check for authentication status
                success: function (data) {
                    var sResult = eval(data);
                    if (typeof (sResult) != "undefined") {
                        if (sResult.Status == "OK")
                            callback(true);
                        else if (sResult.Status == "ERROR")
                            callback(false);
                        else if (sResult.Status == "INVALID")
                            callback(false);
                    }
                    else callback(false);
                },
                error: function (data) {
                    callback(false);
                }
            });
        }
        /*Function to log the gac.push onlick on general links*/
        function TrackLink(sectionCategory, linkAction, linkText) {
            ga('send', 'event', {eventCategory:sectionCategory, eventAction:linkAction, eventLabel:linkText});
        }
